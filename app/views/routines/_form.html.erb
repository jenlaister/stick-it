<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Routine Form</title>
  </head>
  <body>
    <%= form_for @routine do |f| %>
      <%= f.label :routine %>
      <%= f.text_field :routine %>
      <%= f.label :note %>
      <%= f.text_field :note %>
      <%= f.submit "Submit" %>
    <% end %>
  </body>
</html>
