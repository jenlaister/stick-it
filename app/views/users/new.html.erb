<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Sign Up!</h1>
    <%= form_for @user do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username %><br>
      <%= f.label :email %>
      <%= f.text_field :email %><br>
      <%= f.label :password %>
      <%= f.password_field :password %><br>
      <%= f.label "Confirm Password" %>
      <%= f.password_field :password_confirmation %><br>
      <%= f.submit %>
    <% end %>
  </body>
</html>
